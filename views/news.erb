<div class="small-6 small-offset-3 columns">
  <div class="callout primary">
    <h3>Top Stories</h3>
    <p>
      <%@articles.each do |article|%>

      <h5><li><%= article["title"] %></li></h5>
      <li><a href= "%<=article["url"]%>"<%= article["url"] %></a></li>
      <li><%= article["abstract"] %></li>
      <hr></hr>
      <%end%>
    </p>
  </div>
</div>
